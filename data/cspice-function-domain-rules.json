{
  "fallbackDomain": "tbd",
  "domains": [
    {
      "id": "time",
      "title": "Time",
      "description": "Time systems, UTC/ET conversions, SCLK, and time constants."
    },
    {
      "id": "kernels",
      "title": "Kernel management",
      "description": "Kernel bookkeeping: listing, querying, extracting, clearing."
    },
    {
      "id": "kernel-pool",
      "title": "Kernel pool",
      "description": "Kernel pool read/write/query utilities (the shared variable pool)."
    },
    {
      "id": "ids-names",
      "title": "IDs â†” names",
      "description": "Body/frame/surface ID and name translation utilities."
    },
    {
      "id": "frames",
      "title": "Frames & attitude (CK)",
      "description": "Reference frame transformations, attitude representations, and CK routines."
    },
    {
      "id": "ephemeris-spk",
      "title": "Ephemeris (SPK)",
      "description": "SPK access and SPK segment writing/coverage/object utilities (plus closely-related state/orbit helpers)."
    },
    {
      "id": "pck",
      "title": "Planetary constants (PCK)",
      "description": "PCK access and PCK kernel utilities."
    },
    {
      "id": "dsk",
      "title": "Digital shape kernels (DSK)",
      "description": "DSK access, DSK segment writing, and shape model utilities."
    },
    {
      "id": "ek",
      "title": "Event kernels (EK)",
      "description": "EK access, segment building, query, and EK I/O utilities."
    },
    {
      "id": "geometry",
      "title": "Geometry & event finding",
      "description": "Geometric computations (subpoints, intercepts, illumination, occultations, FOV) and GF event finding."
    },
    {
      "id": "coords-vectors",
      "title": "Coordinates, vectors, matrices",
      "description": "Coordinate conversions and core vector/matrix/rotation utilities."
    },
    {
      "id": "cells-windows",
      "title": "Cells & windows",
      "description": "SpiceCell set operations and window operations (WN* routines)."
    },
    {
      "id": "file-io",
      "title": "Low-level file I/O (DAF/DAS/DLA)",
      "description": "Low-level DAF/DAS/DLA architecture routines and file record operations."
    },
    {
      "id": "strings",
      "title": "Strings & parsing",
      "description": "String utilities not otherwise classified (search, replace, word parsing, formatting)."
    },
    {
      "id": "math",
      "title": "Math & numerics",
      "description": "Numerical utilities (Chebyshev polynomials, interpolation, root finding, regression)."
    },
    {
      "id": "error",
      "title": "Error handling",
      "description": "Error system entry/exit, message formatting, and failure-state routines."
    },
    {
      "id": "tbd",
      "title": "TBD (needs review)",
      "description": "Uncertain classification; grouped here for follow-up review."
    }
  ],
  "overrides": {
    "cidfrm": "ids-names",
    "srfc2s": "ids-names",
    "srfcss": "ids-names",
    "srfs2c": "ids-names",
    "srfscc": "ids-names",
    "srfnrm": "geometry",
    "srfrec": "coords-vectors",
    "srfxpt": "geometry",
    "illum_pl02": "geometry",
    "illum_plid_pl02": "geometry"
  },
